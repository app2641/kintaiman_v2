{
  "extends": "airbnb",
  "plugins": [
    "googleappsscript"
  ],
  "env": {
    "googleappsscript/googleappsscript": true,
    "es6": true,
    "mocha": true
  },
  "globals": {
    "global": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "jsx-a11y/href-no-hash": 0,
    "max-len": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
